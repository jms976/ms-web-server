  "scripts": {
    "start": "export PORT=8899 && npm run serve",
    "build": "npm run build-ts && npm run lint && npm run copy-static-assets",
    "build:local": "cp .env.local .env && npm run build-ts && npm run lint && npm run copy-static-assets",
    "build:dev": "cp .env.develop .env && npm run build-ts && npm run lint && npm run copy-static-assets",
    "build:stg": "cp .env.stg .env && npm run build-ts && npm run lint && npm run copy-static-assets",
    "build:product": "cp .env.product .env && npm run build-ts && npm run lint && npm run copy-static-assets",
    "serve": "node dist/bin/www.js",
    "watch-node": "nodemon dist/bin/www.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/bin/www.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "prettier": "prettier \"**/*.{js,ts}\" --write"
  },
